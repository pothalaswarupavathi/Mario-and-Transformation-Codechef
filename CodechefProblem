import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner in=new Scanner(System.in);
		int t= in.nextInt();
		for(int i=0; i<t; i++){
		    int n=in.nextInt();
		    if(n%3 == 0){
		        System.out.println("NORMAL");
		    }
		    else if(n%3 == 1){
		        System.out.println("HUGE");
		    }
		    else{
		        System.out.println("SMALL");
		    }
		}// your code goes here

	}
}
